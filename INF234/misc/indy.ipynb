    "A = [1,8,6,3,6]\n",
    "\n",
    "def compute_P(A, n):\n",
    "    for i in range(1, n + 1):\n",
    "        d[i] = max(d[i - 1], A[i] + d[i - 2])\n",
    "\n",
    "n = len(A) - 1\n",
    "d = [0] * (n + 1)\n",
    "compute_P(A, n)\n",
    "print(d)\n"
   ]
  }
